<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>${env.ARTIFACTORY_REPO_USER}</username>
      <password>${env.ARTIFACTORY_REPO_PASS}</password>
      <id>central</id>
    </server>
    <server>
      <username>${env.ARTIFACTORY_REPO_USER}</username>
      <password>${env.ARTIFACTORY_REPO_PASS}</password>
      <id>snapshots</id>
    </server>
  </servers>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>icets-maven-virtual</name>
          <url>https://infyartifactory.jfrog.io/artifactory/icets-maven-virtual</url>
        </repository>
        <repository>
          <snapshots />
          <id>snapshots</id>
          <name>icets-maven-virtual</name>
          <url>https://infyartifactory.jfrog.io/artifactory/icets-maven-virtual</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>jenkins_plugin</name>
          <url>https://infyartifactory.jfrog.io/artifactory/jenkins_plugin</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>jenkins_plugin</name>
          <url>https://infyartifactory.jfrog.io/artifactory/jenkins_plugin</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
  <proxies>
    <proxy>
        <id>myproxy</id>
        <active>true</active>
        <protocol>http</protocol>
        <host>${env.https_proxy_sv}</host>
        <port>80</port>
        <nonProxyHosts>infygithub.ad.infosys.com</nonProxyHosts>
    </proxy>
	<proxy>
        <id>myproxys</id>
        <active>true</active>
        <protocol>https</protocol>
        <host>${env.https_proxy_sv}</host>
        <port>443</port>
        <nonProxyHosts>infygithub.ad.infosys.com</nonProxyHosts>
    </proxy>
  </proxies>
</settings>
