<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>${env.ARTIFACTORY_REPO_USER}</username>
      <password>${env.ARTIFACTORY_REPO_PASS}</password>
      <id>central</id>
    </server>
    <server>
      <username>${env.ARTIFACTORY_REPO_USER}</username>
      <password>${env.ARTIFACTORY_REPO_PASS}</password>
      <id>snapshots</id>
    </server>
  </servers>
  <proxies>
    <proxy>
      <id>threatpulseid</id>
      <active>true</active>
      <protocol>http</protocol>
      <username>${env.ARTIFACTORY_REPO_USER}</username>
      <password>${env.ARTIFACTORY_REPO_PASS}</password>
      <host>proxy.threatpulse.net</host>
      <port>8080</port>
      <nonProxyHosts></nonProxyHosts>
    </proxy>
  </proxies>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>icets-maven-virtual</name>
          <url>https://infyartifactory.jfrog.io/artifactory/icets-maven-virtual</url>
        </repository>
        <repository>
          <snapshots />
          <id>snapshots</id>
          <name>icets-maven-virtual</name>
          <url>https://infyartifactory.jfrog.io/artifactory/icets-maven-virtual</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>libs-release</name>
          <url>https://infyartifactory.jfrog.io/artifactory/libs-release</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>libs-snapshot</name>
          <url>https://infyartifactory.jfrog.io/artifactory/libs-snapshot</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
